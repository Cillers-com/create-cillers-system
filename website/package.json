{
    "name": "example-spa",
    "version": "1.0.0",
    "description": "Example SPA",
    "main": "index.ts",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "dev": "npm run copy && webpack --watch",
        "build": "npm run clean && npm run copy && webpack",
        "clean": "rm dist/*",
        "copy": "mkdir -p dist && cp index.html app.css config.json dist/"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.4.2",
        "nodemon": "3.0.1",
        "typescript": "^5.1.5",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4"
    },
    "overrides": {
        "tough-cookie": "4.1.3"
    }
}
